<script>
    const passwordInput = document.querySelector('#{{ form.mdp.vars.id }}');
    const showPasswordCheckbox = document.querySelector('#{{ form.showPassword.vars.id }}');

    showPasswordCheckbox.addEventListener('change', function() {
        if (this.checked) {
            passwordInput.type = 'text';
        } else {
            passwordInput.type = 'password';
        }
    });
</script>
<script>
    const passwordInput1 = document.querySelector('#{{ form.generatedPass.vars.id }}');
    const showPasswordCheckbox1 = document.querySelector('#{{ form.showGeneratedPassword.vars.id }}');

    showPasswordCheckbox1.addEventListener('change', function() {
        if (this.checked) {
            passwordInput1.type = 'text';
        } else {
            passwordInput1.type = 'password';
        }
    });
</script>
<script>
    const generPass = document.querySelector('#{{ form.generatedPass.vars.id }}');
    const button = document.querySelector('#my-button');
    const email = document.querySelector('#{{ form.login.vars.id }}');

    button.addEventListener('click', () => {
        const parts = email.value.split('@');
        const emailPart = parts[0];
        const password = crypto.getRandomValues(new Uint8Array(2)).toString().replace(/,/g, '');

        generPass.value =  emailPart + password ;
    });
</script>
